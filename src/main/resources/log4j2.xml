<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} %-5p[%c] %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root>
        </Root>
        <Logger name="SeleniumTest" level="info">
            <AppenderRef ref="Console"/>
        </Logger>
    </Loggers>
</Configuration>